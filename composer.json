{
    "name": "sanlilin/laravel-plugin",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "wanghongbin",
            "email": "wanghongbin816@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Sanlilin\\LaravelPlugin\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Sanlilin\\LaravelPlugin\\Tests\\": "tests",
            "PluginsTest\\": "tests/stubs/valid/"
        }
    },
    "require": {
        "php": "^8.0",
        "ext-zip": "*",
        "ext-json": "*",
        "laravel/framework": "^11.0",
        "artesaos/seotools": "^v1.3",
        "mcamara/laravel-localization": "^1.7",
        "illuminate/config": "^10.0 || ^11.0 || ^12.0",
        "illuminate/support": "^10.0 || ^11.0 || ^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0 || ^10.0 || ^11.5.3",
        "orchestra/testbench": "^8.0 || ^9.0 || ^10.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sanlilin\\LaravelPlugin\\Providers\\PluginServiceProvider"
            ]
        }
    }
}
